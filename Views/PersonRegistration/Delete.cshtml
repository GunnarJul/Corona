@model CoranaRegistration.Models.PersonRegistration

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 

<h3>Bekræft sletning</h3>
<div>
    <h4>Person Registration</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2" hidden >
            @Html.DisplayNameFor(model => model.id)
        </dt>
        <dd class="col-sm-10" hidden>
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.fullname)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.fullname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.adress)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.adress)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.cpr)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.cpr)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.zipcode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.zipcode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.testresult)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.testresult)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Tilbage</a>
    </form>
</div>
